<body onload="getMes()">

    <div id="header">
        <ul class="layui-nav">
            <li class="layui-nav-item"><a href="">评分行为表</a></li>
        </ul>
        <div class="nav5" onclick="window.open('./mainPannel.html','_self')"><img src="../img/ppp.png" title="返回"></div>
    </div>

    <div id="middle">
        <div>
            <form class="layui-form" action="">
                <div class="layui-inline">
                    <label class="layui-form-label">用户名</label>
                    <div class="layui-input-inline-block">
                        <input id="userName" type="text" readonly="readonly" name="userName" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">创建时间</label>
                    <div class="layui-input-inline-block">
                        <input id="createTime" type="text" readonly="readonly" name="createTime" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">到期时间</label>
                    <div class="layui-input-inline-block">
                        <input id="expireTime" type="text" readonly="readonly" name="expireTime" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">联系方式</label>
                    <div class="layui-input-inline-block">
                        <input id="phoneNumber" type="text" readonly="readonly" name="phoneNumber" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">联系邮箱</label>
                    <div class="layui-input-inline-block">
                        <input id="email" type="text" readonly="readonly" name="email" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">客栈名称</label>
                    <div class="layui-input-inline-block">
                        <input id="hotelName" type="text" readonly="readonly" name="hotelName" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">客栈等级</label>
                    <div class="layui-input-inline-block">
                        <input id="hotelGrade" type="text" readonly="readonly" name="hotelGrade" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">登录等级</label>
                    <div class="layui-input-inline-block">
                        <input id="loginRole" type="text" readonly="readonly" name="loginRole" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">角色等级</label>
                    <div class="layui-input-inline-block">
                        <input id="loginRoleLevel" type="text" readonly="readonly" name="loginRoleLevel" required lay-verify="required" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
            </form>
        </div>
    </div>


    <div id="footer">
        <div class="layui-tab">
            <ul class="layui-tab-title">
                <li class="layui-this">个人评分</li>
                <!-- <li>承租</li>
                <li>销售及月流水情况</li>
                <li>经营规模</li>
                <li>信用评分</li>
                <li>个人人脉评分</li> -->
            </ul>
            <!--form-->
            <div class="layui-tab-content">
                <!-- 个人人脉评分（满分100） -->
                <div class="layui-tab-item layui-show">
                    <form class="layui-form" action="">
                        <div class="layui-form-item">
                            <label class="layui-form-label">个人总评分(满分100)</label>
                            <div class="layui-input-block">
                                <input type="text" name="idproofScoring" required lay-verify="required" placeholder="请输入分数" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">承租总评分(满分100)</label>
                            <div class="layui-input-block">
                                <input type="text" name="tenantRatingScoring" required lay-verify="required" placeholder="请输入分数" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">销量总评分(满分100)</label>
                            <div class="layui-input-block">
                                <input type="text" name="salesScore" required lay-verify="required" placeholder="请输入分数" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">规模总评分(满分100)</label>
                            <div class="layui-input-block">
                                <input type="text" name="salesScoring" required lay-verify="required" placeholder="请输入分数" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">信用总评分(满分100)</label>
                            <div class="layui-input-block">
                                <input type="text" name="totalCreditScore" required lay-verify="required" placeholder="请输入分数" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">人脉总评分(满分100)</label>
                            <div class="layui-input-block">
                                <input type="text" name="resourceScoring" required lay-verify="required" placeholder="请输入分数" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <div class="layui-input-block">
                                <button class="layui-btn" lay-submit lay-filter="formDemo">上传信息</button>
                                <button type="reset" class="layui-btn layui-btn-primary">重置</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
